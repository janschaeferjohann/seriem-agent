<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:kwsoft:mtext:tonic:simpledom" targetNamespace="urn:kwsoft:mtext:tonic:simpledom" elementFormDefault="qualified">
	
	<!-- Enum -->	
	<xs:simpleType name="AnchorSideEnumType">
	 <xs:restriction base="xs:string">
	 	<xs:enumeration value="opposite">
	 	<xs:annotation><xs:documentation>Opposite border of the paper, i.e. right paper edge when
defining the left edge anchor, left edge when defining the right edge
anchor, and so on.</xs:documentation></xs:annotation>
	 	</xs:enumeration>
	 	<xs:enumeration value="same">
	 	<xs:annotation><xs:documentation>Same border of the paper, i.e. left edge when defining
the left edge anchor, right edge when defining the right edge anchor,
and so on.</xs:documentation></xs:annotation>
	 	</xs:enumeration>
  	 </xs:restriction>
	</xs:simpleType>
	<!-- /Enum -->
	
</xs:schema>