<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:kwsoft:mtext:tonic:simpledom" targetNamespace="urn:kwsoft:mtext:tonic:simpledom" elementFormDefault="qualified">
	
	<!-- Enum -->	
	<xs:simpleType name="HorizontalOverflowEnumType">
	 <xs:restriction base="xs:string">
	 	<xs:enumeration value="error">
	 	<xs:annotation><xs:documentation>Error is thrown when content exceeds horizontal boundary of element with a fixed width</xs:documentation></xs:annotation>
	 	</xs:enumeration>
	 	<xs:enumeration value="truncate">
	 	<xs:annotation><xs:documentation>Content is truncated when content exceeds horizontal boundary of element with a fixed width</xs:documentation></xs:annotation>
	 	</xs:enumeration>
  	 </xs:restriction>
	</xs:simpleType>
	<!-- /Enum -->
	
</xs:schema>