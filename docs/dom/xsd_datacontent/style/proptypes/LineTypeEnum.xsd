<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:kwsoft:mtext:tonic:simpledom" targetNamespace="urn:kwsoft:mtext:tonic:simpledom" elementFormDefault="qualified">
	
	<!-- Enum -->	
	<xs:simpleType name="LineTypeEnumType">
	 <xs:restriction base="xs:string">
	 	<xs:enumeration value="none" />
	 	<xs:enumeration value="dashed" />
	 	<xs:enumeration value="dotted" />
	 	<xs:enumeration value="solid">
	 	<xs:annotation><xs:documentation>Single solid line.</xs:documentation></xs:annotation>
	 	</xs:enumeration>
	 	<xs:enumeration value="double">
	 	<xs:annotation><xs:documentation>Two solid lines.</xs:documentation></xs:annotation>
	 	</xs:enumeration>
	 	<xs:enumeration value="triple">
	 	<xs:annotation><xs:documentation>Three solid lines.</xs:documentation></xs:annotation>
	 	</xs:enumeration>
	 	<xs:enumeration value="four">
	 	<xs:annotation><xs:documentation>Four solid lines.</xs:documentation></xs:annotation>
	 	</xs:enumeration>
  	 </xs:restriction>
	</xs:simpleType>
	<!-- /Enum -->
	
</xs:schema>