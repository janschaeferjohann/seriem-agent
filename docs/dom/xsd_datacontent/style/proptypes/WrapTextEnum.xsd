<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:kwsoft:mtext:tonic:simpledom" targetNamespace="urn:kwsoft:mtext:tonic:simpledom" elementFormDefault="qualified">
	
	<!-- Enum -->	
	<xs:simpleType name="WrapTextEnumType">
	 <xs:restriction base="xs:string">
	 	<xs:enumeration value="after">
	 	<xs:annotation><xs:documentation>The default, following content is placed after the space occupied by the image</xs:documentation></xs:annotation>
	 	</xs:enumeration>
	 	<xs:enumeration value="through">
	 	<xs:annotation><xs:documentation>Image is printed behind text</xs:documentation></xs:annotation>
	 	</xs:enumeration>
	 	<xs:enumeration value="start">
	 	<xs:annotation><xs:documentation>Text flows at the start side of the image, i.e. in the left-to-right writing, the image is aligned to the right side and text flows at its left side.</xs:documentation></xs:annotation>
	 	</xs:enumeration>
	 	<xs:enumeration value="end">
	 	<xs:annotation><xs:documentation>Text flows at the end side of the image, i.e. in the left-to-right writing, image is placed at the left side with text flowing around its right side.</xs:documentation></xs:annotation>
	 	</xs:enumeration>
  	 </xs:restriction>
	</xs:simpleType>
	<!-- /Enum -->
	
</xs:schema>